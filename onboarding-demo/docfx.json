{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/onboarding-demo/breadcrumb/toc.json",
      "extendBreadcrumb": true,
            "feedback_system": "GitHub",
      "feedback_github_repo": "jmazzotta/onboarding-demo",
      "feedback_product_url": "http://google.com/"    
    },
    "fileMetadata": {
       "feedback_system": {
             "docs-ref-autogen/**/*": "None"}
    },
    "template": [],
    "dest": "onboarding-demo",
    "markdownEngineName": "markdig"
  }
}